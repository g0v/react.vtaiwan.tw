{
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": 0,
    "quotes": 0,
    "semi": 0,
    "camelcase": 0,
    "no-empty": 0,
    "no-loop-func": 0,
    "no-multi-spaces": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "no-process-exit": 0
  }
}
